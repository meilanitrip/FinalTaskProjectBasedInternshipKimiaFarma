CREATE TABLE kimia_farma.analisa_transaksi
AS SELECT
    t.transaction_id,
    t.date,
    c.branch_id,
    c.branch_name,
    c.kota,
    c.provinsi,
    SUM(t.price) AS total_sales
FROM
    rakamin-kf-analytics-418902.kimia_farma.kf_final_transaction t
JOIN
    kimia_farma.kf_kantor_cabang c
ON
    t.branch_id = c.branch_id
WHERE
    t.date BETWEEN '2020-01-01' AND '2023-12-31'
GROUP BY
    t.transaction_id,
    t.date,
    c.branch_id,
    c.branch_name,
    c.kota,
    c.provinsi
ORDER BY
    total_sales DESC;
